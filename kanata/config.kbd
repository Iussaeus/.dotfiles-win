(defcfg
windows-altgr cancel-lctl-press
process-unmapped-keys yes
linux-dev-names-include (
"AT Translated Set 2 keyboard"
	)
)

(defsrc
  caps
  lalt
  ralt
)

(defalias
  escctrl (tap-hold 150 100 esc lctl)
  altenter (tap-hold 100 100 enter lalt)
  altbackspace (tap-hold 100 100 bspc ralt)
)

(deflayer base
  @escctrl
  @altenter
  @altbackspace
)
